
#include <iostream>

using namespace std;

int main()
{
    int row, k=1;
    cin>>row;
   
   
    for(int i = 0; i < row; i++)
    {
        for(int j=0; j< row; j++)
        {
            if(i+j >= row )
            {
              
                cout<<k;
                k++;
               
            }
            cout<<"\t";
        }
        cout<<"\n";
    }
}
